{
  "manifest_version": 3,
  "name": "Tarayıcı Temizleyici Pro",
  "version": "1.0.0",
  "description": "Tarayıcı verilerinizi otomatik veya manuel olarak temizleyin. Geçmiş, çerezler, önbellek ve daha fazlası!",
  "author": "Kullanıcı",
  "permissions": [
    "browsingData",
    "storage",
    "alarms"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Tarayıcı Temizleyici Pro"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

